<div id="wrapper">
  <ul class="savedLi" id="articles">

    {{#each data}}

    <li class="newList"><a href="{{this.href}}">{{this.title}}</a></li>

    <button class="button savebtn btn btn-danger" type="button" id="{{this._id}}">Remove</button>

    <!-- Button trigger modal -->
    <button type="button" class="btn btn-primary noteBtn" data-toggle="modal" data-target="#exampleModal">
      add a note
    </button>

    {{/each}}

  </ul>
  <div id="notes"></div>
</div>

