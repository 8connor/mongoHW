<div id="wrapper">
  <ul id="articles">

    {{#each articleList}}

    <li class="newList"><a href="{{this.href}}">{{this.title}}</a></li>

    <button class="button btn btn-info savebtn" type="button" id="{{this._id}}">Save</button>

    {{/each}}
    
  </ul>
  <div id="notes"></div>
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/app.js"></script>